{
    "name": "outtimes/dujiaoka",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "filament/filament": "^3.2",
        "filament/spatie-laravel-settings-plugin": "^3.3",
        "geoip2/geoip2": "^2.13",
        "jenssegers/agent": "^2.6",
        "laravel/framework": "^12.0",
        "laravel/tinker": "^2.8",
        "mews/captcha": "^3.0",
        "mgcodeur/laravel-currency-converter": "^1.0",
        "paypal/paypal-server-sdk": "^0.6",
        "simplesoftwareio/simple-qrcode": "^4.0",
        "stripe/stripe-php": "^7.84",
        "xhat/payjs-laravel": "^1.6",
        "yansongda/pay": "^3.2"
    },
    "require-dev": {
        "spatie/laravel-ignition": "^2.0",
        "fakerphp/faker": "^1.23",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "phpunit/phpunit": "^10.5|^11.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform-check": false
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/Helpers/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "@php artisan filament:upgrade"
        ]
    }
}
